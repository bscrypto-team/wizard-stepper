{
  "name": "@bscrypto-team/wizzard-stepper",
  "version": "0.1.7-pre.0",
  "description": "Wizzard stepper",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "repository": "git@github.com:bscrypto-team/wizzard-stepper.git",
  "author": "Andr√©s Paredes <paredesandres235@gmail.com>",
  "license": "MIT",
  "files": [
    "/build",
    "/assets",
    "/@types"
  ],
  "publishConfig": {
    "@bscrypto-team:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "prebuild": "install-peers",
    "build": "tsc --declaration && tsc-alias",
    "typescript": "tsc --noEmit",
    "lint": "eslint .",
    "prettier:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\" ",
    "prettier:check": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|css|md)\" ",
    "pretty-quick:fix": "pretty-quick --staged --patern \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "release:setup": "yarn config set version-git-tag true && yarn config set version-tag-prefix release-v",
    "release:patch": "yarn release:setup && yarn version --patch",
    "release:minor": "yarn release:setup && yarn version --minor",
    "release:major": "yarn release:setup && yarn version --major",
    "release:pre:major": "yarn release:setup && yarn version --premajor --preid pre",
    "release:pre:minor": "yarn release:setup && yarn version --preminor --preid pre",
    "release:pre:patch": "yarn release:setup && yarn version --prepatch --preid pre",
    "release:pre:release": "yarn release:setup && yarn version --prerelease --preid pre",
    "husky-install": "husky install",
    "postversion": "bash bin/pushTag.sh"
  },
  "peerDependencies": {
    "react": ">= 17.0.2 < 18.0.0",
    "react-dom": ">= 17.0.2 < 18.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.185",
    "@types/react": "17.0.35",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "install-peers-cli": "^2.2.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.3",
    "utility-types": "^3.10.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
